FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0/air";
    object      cellToRegion;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    inlet
    {
        type            zeroGradient;
    }
    BaseAndTop
    {
        type            empty;
    }
    air_to_porous
    {
        type            calculated;
        value           uniform 0;
    }
    symm
    {
        type            symmetry;
    }
}


